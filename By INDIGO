local statsTab = window:AddTab("Stats")
local statsFolder = statsTab:AddFolder("Estadísticas en Tiempo Real")

local sessionStartTime = tick()
local rebirthsGained = 0
local lastRebirths = player.leaderstats.Rebirths.Value

local strengthLabel = statsFolder:AddLabel("Fuerza: Cargando...")
local durabilityLabel = statsFolder:AddLabel("Durabilidad: Cargando...")
local rebirthsLabel = statsFolder:AddLabel("Renacimientos: Cargando...")
local killsLabel = statsFolder:AddLabel("Asesinatos: Cargando...")
local timeLabel = statsFolder:AddLabel("Tiempo de sesión: 0s")
local rphLabel = statsFolder:AddLabel("Renacimientos/hora: Calculando...")

local function updateStats()
    local strength = player.leaderstats.Strength.Value
    local durability = player.leaderstats.Durability.Value
    local rebirths = player.leaderstats.Rebirths.Value
    local kills = player.leaderstats.Kills.Value
    local elapsed = tick() - sessionStartTime

    strengthLabel.Text = "Fuerza: "..string.format("%.2f", strength)
    durabilityLabel.Text = "Durabilidad: "..tostring(durability)
    rebirthsLabel.Text = "Renacimientos: "..tostring(rebirths).." (+"..tostring(rebirthsGained)..")"
    killsLabel.Text = "Asesinatos: "..tostring(kills)

    local hours = elapsed / 3600
    local rph = hours > 0 and (rebirthsGained / hours) or 0
    rphLabel.Text = "Renacimientos/hora: "..string.format("%.2f", rph)

    local timeStr = string.format("%02d:%02d:%02d", math.floor(elapsed/3600), math.floor((elapsed%3600)/60), math.floor(elapsed%60))
    timeLabel.Text = "Tiempo de sesión: "..timeStr
end

spawn(function()
    while task.wait(1) do
        local current = player.leaderstats.Rebirths.Value
        if current > lastRebirths then
            rebirthsGained = rebirthsGained + (current - lastRebirths)
            lastRebirths = current
        end
        updateStats()
    end
end)

statsFolder:AddButton("Reset Stats", function()
    sessionStartTime = tick()
    rebirthsGained = 0
    lastRebirths = player.leaderstats.Rebirths.Value
    updateStats()
end)

statsFolder:AddButton("Copiar Estadísticas", function()
    local stats = [[
HONNYDEZZ STATS
Fuerza: ]]..string.format("%.2f", player.leaderstats.Strength.Value)..[[
Durabilidad: ]]..tostring(player.leaderstats.Durability.Value)..[[
Renacimientos: ]]..tostring(player.leaderstats.Rebirths.Value)..[[
Asesinatos: ]]..tostring(player.leaderstats.Kills.Value)..[[
Tiempo: ]]..timeLabel.Text
    setclipboard(stats)
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "Estadísticas copiadas",
        Text = "Todo listo al portapapeles!",
        Duration = 3
    })
end)

local killerTab = window:AddTab("Killer")
local killerFolder = killerTab:AddFolder("Kill Aura")

local killWhitelist = {}

killerFolder:AddTextBox("Whitelist Player", function(name)
    local plr = game.Players:FindFirstChild(name)
    if plr then
        table.insert(killWhitelist, plr.UserId)
        game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "Whitelist",
            Text = name.." añadido a la whitelist",
            Duration = 3
        })
    end
end)

killerFolder:AddButton("Clear Whitelist", function()
    killWhitelist = {}
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "Whitelist",
        Text = "Whitelist limpiada",
        Duration = 3
    })
end)

killerFolder:AddSwitch("Kill Aura (Todos)", function(bool)
    getgenv().killAura = bool
    spawn(function()
        while getgenv().killAura do
            task.wait(0.05)
            for _,v in pairs(game.Players:GetPlayers()) do
                if v ~= game.Players.LocalPlayer and not table.find(killWhitelist, v.UserId) and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
                    game.Players.LocalPlayer.muscleEvent:FireServer("punch","rightHand")
                    game.Players.LocalPlayer.muscleEvent:FireServer("punch","leftHand")
                end
            end
        end
    end)
end)
local miscTab = window:AddTab("Misc")

miscTab:AddButton("Walk on Water", function()
    local part = Instance.new("Part")
    part.Size = Vector3.new(1000, 1, 1000)
    part.Position = Vector3.new(0, -3, 0)
    part.Anchored = true
    part.Transparency = 0.7
    part.CanCollide = true
    part.Material = Enum 256 -- Water material
    part.Color = Color3.fromRGB(0, 162, 255)
    part.Parent = workspace
end)

miscTab:AddButton("Remove All Portals", function()
    for _,v in pairs(workspace:GetDescendants()) do
        if v.Name:find("Portal") or v.Name:find("Ad") then
            v:Destroy()
        end
    end
end)

miscTab:AddSwitch("NoClip", function(bool)
    getgenv().noclip = bool
    local char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
    local hum = char:WaitForChild("Humanoid")
    
    if bool then
        local function onStepped()
            if getgenv().noclip then
                for _,part in pairs(char:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end
        game:GetService("RunService").Stepped:Connect(onStepped)
    end
end)

miscTab:AddSwitch("Infinite Jump", function(bool)
    getgenv().infJump = bool
    game:GetService("UserInputService").JumpRequest:Connect(function()
        if getgenv().infJump then
            game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
        end
    end)
end)

