local rebirthsFolder = farmPlusTab:AddFolder("Rebirths")

local targetRebirthValue = 1000

rebirthsFolder:AddTextBox("Rebirth Target", function(text)
    local newValue = tonumber(text)
    if newValue and newValue > 0 then
        targetRebirthValue = newValue
        game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "Objetivo Actualizado",
            Text = "Nuevo objetivo: "..tostring(targetRebirthValue).." renacimientos",
            Duration = 5
        })
    end
end)

local infiniteSwitch
local targetSwitch = rebirthsFolder:AddSwitch("Auto Rebirth Target", function(bool)
    _G.targetRebirthActive = bool
    if bool then
        if _G.infiniteRebirthActive and infiniteSwitch then
            infiniteSwitch:Set(false)
            _G.infiniteRebirthActive = false
        end
        spawn(function()
            while _G.targetRebirthActive do
                wait(0.1)
                if player.leaderstats.Rebirths.Value >= targetRebirthValue then
                    targetSwitch:Set(false)
                    _G.targetRebirthActive = false
                    game:GetService("StarterGui"):SetCore("SendNotification",{
                        Title = "Objetivo Alcanzado!",
                        Text = "Has llegado a "..targetRebirthValue.." renacimientos",
                        Duration = 10
                    })
                    break
                end
                ReplicatedStorage.rEvents.rebirthRemote:InvokeServer("rebirthRequest")
            end
        end)
    end
end)

infiniteSwitch = rebirthsFolder:AddSwitch("Auto Rebirth (Infinite)", function(bool)
    _G.infiniteRebirthActive = bool
    if bool then
        if _G.targetRebirthActive and targetSwitch then
            targetSwitch:Set(false)
            _G.targetRebirthActive = false
        end
        spawn(function()
            while _G.infiniteRebirthActive do
                wait(0.1)
                ReplicatedStorage.rEvents.rebirthRemote:InvokeServer("rebirthRequest")
            end
        end)
    end
end)
local autoEquipToolsFolder = farmPlusTab:AddFolder("Auto Equip Tools")

autoEquipToolsFolder:AddButton("Gamepass AutoLift", function()
    local owned = Instance.new("Folder", game.Players.LocalPlayer)
    owned.Name = "ownedGamepasses"
    local auto = Instance.new("IntValue", owned)
    auto.Name = "AutoLift"
    auto.Value = 1
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "Gamepass Desbloqueado",
        Text = "AutoLift activado gratis!",
        Duration = 5
    })
end)

autoEquipToolsFolder:AddSwitch("Auto Weight", function(bool)
    getgenv().autoWeight = bool
    spawn(function()
        while getgenv().autoWeight do
            task.wait()
            for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if v.Name == "Weight" then
                    v.Parent = game.Players.LocalPlayer.Character
                end
            end
        end
    end)
end)

autoEquipToolsFolder:AddSwitch("Auto Pushups", function(bool)
    getgenv().autoPush = bool
    spawn(function()
        while getgenv().autoPush do
            task.wait()
            for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if v.Name == "Pushups" then
                    v.Parent = game.Players.LocalPlayer.Character
                    game.Players.LocalPlayer.muscleEvent:FireServer("rep")
                end
            end
        end
    end)
end)

