FarmTab:AddToggle({
    Name = "Jungle Gym (Bench + Squat + Pull + Boulder)",
    Callback = function(v)
        getgenv().JungleGym = v
        spawn(function()
            while getgenv().JungleGym do
                for _, pos in pairs({
                    CFrame.new(-8173, 64, 1898),
                    CFrame.new(-8352, 34, 2878),
                    CFrame.new(-8666, 34, 2070),
                    CFrame.new(-8621, 34, 2684)
                }) do
                    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        LocalPlayer.Character.HumanoidRootPart.CFrame = pos
                        task.wait(0.3)
                        VirtualInputManager:SendKeyEvent(true, "E", false, game)
                        task.wait(0.1)
                        VirtualInputManager:SendKeyEvent(false, "E", false, game)
                        repeat
                            pcall(function() LocalPlayer.muscleEvent:FireServer("rep") end)
                            task.wait()
                        until not getgenv().JungleGym
                    end
                end
            end
        end)
    end
})

-- === BRAWL TAB ===
BrawlTab:AddToggle({
    Name = "Auto Win Brawl (OP)",
    Callback = function(v)
        getgenv().AutoWinBrawl = v
        spawn(function()
            while getgenv().AutoWinBrawl do
                pcall(function()
                    if LocalPlayer.PlayerGui.gameGui.brawlJoinLabel.Visible then
                        ReplicatedStorage.rEvents.brawlEvent:FireServer("joinBrawl")
                    end
                end)
                task.wait(0.5)
            end
        end)
        spawn(function()
            while getgenv().AutoWinBrawl do
                LocalPlayer.muscleEvent:FireServer("punch", "rightHand")
                LocalPlayer.muscleEvent:FireServer("punch", "leftHand")
                task.wait(0.05)
            end
        end)
        spawn(function()
            while getgenv().AutoWinBrawl do
                for _, p in pairs(Players:GetPlayers()) do
                    if p ~= LocalPlayer and p.Character then
                        pcall(function()
                            firetouchinterest(p.Character.HumanoidRootPart, LocalPlayer.Character.LeftHand, 0)
                            firetouchinterest(p.Character.HumanoidRootPart, LocalPlayer.Character.RightHand, 0)
                            task.wait()
                            firetouchinterest(p.Character.HumanoidRootPart, LocalPlayer.Character.LeftHand, 1)
                            firetouchinterest(p.Character.HumanoidRootPart, LocalPlayer.Character.RightHand, 1)
                        end)
                    end
                end
                task.wait(0.05)
            end
        end)
    end
})

